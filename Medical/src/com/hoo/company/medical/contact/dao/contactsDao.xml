<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hoo.company.medical.contact.dao.IContactsDao">
	
   <select id="queryLt" resultType="com.hoo.company.medical.contact.entity.Contacts">
   		SELECT 
			NAME     AS "name",
			TEL      AS "tel",
			HOME_TEL AS "homeTel"	
  		FROM CONTACTS
  		
   </select>
    
</mapper>

